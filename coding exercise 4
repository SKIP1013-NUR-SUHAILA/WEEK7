package monday_exercise;

class Looping_2_Decrement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int k= 20, j= 20, l;
		
		while (k > 10) {
	        System.out.println("k= "+ k);
	          k--; //mean k= k-1
		}      
	    do {
		    System.out.println("j= "+ j);
		    j--;
	    } while (j>10);
	    
	    for(l=20; l>10; l--) {
	    	 System.out.println("l= "+ l);
	    }
	}

}
